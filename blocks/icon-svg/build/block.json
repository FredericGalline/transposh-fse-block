{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/icon-svg",
  "version": "0.1.0",
  "title": "Icon Svg",
  "category": "jetstream",
  "icon": "smiley",
  "description": "Block for rendering custom SVG icons.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "svgContent": {
      "type": "string",
      "default": "Your SVG content here"
    },
    "width": {
      "type": "number",
      "default": 200
    },
    "svgColor": {
      "type": "string",
      "default": "#000000"
    },
    "useCurrentColor": {
      "type": "boolean",
      "default": true
    },
    "align": {
      "type": "string",
      "default": "none"
    },
    "margin": {
      "type": "number",
      "default": 10
    },
    "padding": {
      "type": "number",
      "default": 10
    },
    "useBackground": {
      "type": "boolean",
      "default": false
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "link": {
      "type": "object",
      "default": {
        "url": "",
        "opensInNewTab": false
      }
    }
  },
  "textdomain": "icon-svg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}